{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "name": {
      "type": "string",
      "unique": true
    },
    "company": {
      "type": "enumeration",
      "enum": [
        "Saf Team"
      ]
    },
    "jobTitle": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "linkedIn": {
      "type": "string",
      "unique": false,
      "regex": "https?:\\/\\/(?:www\\.|(?!www))linkedin\\.[^\\s]{2,}"
    },
    "twitter": {
      "type": "string",
      "unique": false,
      "regex": "https?:\\/\\/(?:www\\.|(?!www))twitter\\.[^\\s]{2,}"
    },
    "facebook": {
      "type": "string",
      "regex": "https?:\\/\\/(?:www\\.|(?!www))facebook\\.[^\\s]{2,}"
    },
    "instagram": {
      "type": "string",
      "regex": "https?:\\/\\/(?:www\\.|(?!www))instagram\\.[^\\s]{2,}"
    },
    "displayEmail": {
      "type": "email"
    }
  }
}
